<div class="container">
  <div class="mb-4 mt-5">
    <h3>会員情報一覧</h3>
  </div>
  <div class="row">
    <div class="col-8 mx-auto">
      <table class="table">
        <thead class="table-dark">
          <th>ID</th>
          <th>名前</th>
          <th>会員ステータス</th>
          <th></th>
        </thead>
      <% @customers.each do |customer| %>
        <tr>
          <td><%=customer.id %></td>
          <td><%=customer.name%></td>
          <td><%=customer.is_deleted %></td>
          <td><%=link_to "編集する",edit_admin_customer_path(customer),class:"btn btn-outline-success" %></td>
        </tr>
      <% end %>
      </table>
    </div>
  </div>
</div>





